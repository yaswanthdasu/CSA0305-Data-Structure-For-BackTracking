#include <stdio.h>
int main() {
    int a[100], n, key, low=0, high, mid;
    printf("Enter size (sorted array): ");
    scanf("%d", &n);
    printf("Enter elements:\n");
    for(int i=0;i<n;i++)
        scanf("%d", &a[i]);
    printf("Enter key: ");
    scanf("%d", &key);
    high = n - 1;
    while(low <= high) {
        mid = (low + high)/2;
        if(a[mid] == key) {
            printf("Found at index %d\n", mid);
            return 0;
        } else if(a[mid] < key)
            low = mid + 1;
        else
            high = mid - 1;
    }
    printf("Not found\n");
    return 0;
}
