#include <stdio.h>

int main() {
    int n, a=0, b=1, sum=0, next, i;
    printf("Enter number of terms: ");
    scanf("%d", &n);

    for(i=0; i<n; i++) {
        sum += a;
        next = a + b;
        a = b;
        b = next;
    }
    printf("Sum of Fibonacci series: %d\n", sum);
    return 0;
}
