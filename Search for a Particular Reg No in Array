#include <stdio.h>

int main() {
    int arr[100], n, key, i, found=0;
    printf("Enter number of reg nos: ");
    scanf("%d", &n);
    printf("Enter reg nos:\n");
    for(i=0; i<n; i++) scanf("%d", &arr[i]);
    printf("Enter reg no to search: ");
    scanf("%d", &key);

    for(i=0; i<n; i++) {
        if(arr[i] == key) {
            found = 1;
            break;
        }
    }
    printf(found ? "Reg no found at index %d\n" : "Reg no not found\n", i);
    return 0;
}
