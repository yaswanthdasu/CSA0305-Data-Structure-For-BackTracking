#include <stdio.h>
#include <ctype.h>

int isValidString(char str[]) {
    for(int i=0; str[i] != '\0'; i++) {
        if(!isalpha(str[i]))
            return 0;
    }
    return 1;
}

int main() {
    char str[100];
    printf("Enter string: ");
    scanf("%s", str);
    printf("%s\n", isValidString(str) ? "Valid" : "Invalid");
    return 0;
}
