#include <stdio.h>

int main() {
    int arr[100], n, key, i;
    printf("Enter size: "); scanf("%d", &n);
    printf("Enter elements:\n");
    for(i=0; i<n; i++) scanf("%d", &arr[i]);
    printf("Enter element to find: "); scanf("%d", &key);

    for(i=0; i<n; i++) {
        if(arr[i] == key) {
            printf("Element found at index %d\n", i);
            return 0;
        }
    }
    printf("Element not found\n");
    return 0;
}
